{
  "backgroundEffects": {
    "enabled": true,
    "animations": {
      "subtleFloat": {
        "duration": "15s",
        "enabled": true
      },
      "gentleRotate": {
        "duration": "120s", 
        "enabled": true
      },
      "blurPulse": {
        "duration": "20s",
        "enabled": true
      }
    },
    "themes": {
      "elegant": {
        "backgroundImage": "/images/restaurant-elegant-bg.svg",
        "overlay": "rgba(0, 0, 0, 0.3)",
        "blur": "1px"
      },
      "parquet": {
        "backgroundImage": "/images/restaurant-parquet-bg.svg", 
        "overlay": "rgba(0, 0, 0, 0.6)",
        "blur": "2px"
      }
    },
    "currentTheme": "elegant",
    "textVisibility": {
      "textShadow": "0 0 2px rgba(255, 255, 255, 0.8)",
      "backgroundColor": "rgba(255, 255, 255, 0.9)",
      "zIndex": 100,
      "padding": "0.25rem 0.5rem",
      "borderRadius": "4px"
    },
    "responsiveSettings": {
      "mobile": {
        "animationDuration": "60s",
        "reducedEffects": true
      }
    }
  },
  "accessibility": {
    "highContrast": false,
    "reducedMotion": false,
    "textEnhancement": true
  },
  "performance": {
    "enableGpuAcceleration": true,
    "optimizeForMobile": true
  }
}
